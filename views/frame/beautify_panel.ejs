<!-- 美化效果面板 -->
    <h3><span>美化效果</span></h3>
    <div class="line_num"><a class="on" tab="0">外观</a><a tab="1">推荐</a></div>
    <div class="line_con" style="width:508px;left:0;">
        <div class="out" id="a1">
            <div class="inner" id="b1">
                <div>
                    <div id="picker"></div>
                    <div class="change_color">
                        <strong><em>主色</em></strong>
                        <p><input type="text" value="<%-commonjson.pageColor.mainColor%>" class="colorwell color_radio_0"></p>
                    </div>
                    <div class="change_color">
                        <strong><em>副色</em></strong>
                        <p><input type="text" value="<%-commonjson.pageColor.deputyColor%>" class="colorwell color_radio_1"></p>
                    </div>
                    <div class="change_color bg_set column_upload">
                            <div class="bg_color_img"><a class="on">栏目背景</a><a>栏目背景图</a></div>
                            <div class="bg_style"><p><input type="text" value="<%-commonjson.pageColor.columnColor%>"  class="colorwell color_radio_2"></p></div>
                            <div class="bg_style bgStyle_set">
                                <div class="up_img">
                                    <div class="show_img"><i class="fa fa-camera"></i><% if ( commonjson.pageColor.backgroundImages != undefined || commonjson.pageColor.backgroundImages != '' ) { %><img src="<%-commonjson.pageColor.backgroundImages%>"  class="img" /> <% } %></div>
                                    <div class="up_but"><label><a  class="upload"><i class="fa fa-camera"></i>上传<input type="file" name="filename" accept="image/gif,image/jpeg,image/jpg,image/png"></a></label><a class="eliminate"><i class="fa fa-times"></i>清除</a></div>
                                </div>
                                <div class="bg_position">
                                    <h3><span>背景位置</span></h3>
                                    <div class="location_sele clearfix">
                                        <a class="posi_top on"><em></em><input type="radio" name="bj_img_posi" ></a>
                                        <a class="posi_middle"><em></em><input type="radio" name="bj_img_posi" checked></a>
                                        <a class="posi_bottom"><em></em><input type="radio" name="bj_img_posi"></a>
                                    </div>
                                </div>
                                <div class="repeat">
                                    <span><em>不重复</em><i class="fa fa-sort-down"></i></span>
                                    <ul>
                                        <li><a class="repeat_btn" data-repeat="no-repeat">不重复</a></li>
                                        <li><a class="repeat_btn" data-repeat="repeat-x">水平方向重复</a></li>
                                        <li><a class="repeat_btn" data-repeat="repeat-y">垂直方向重复</a></li>
                                        <li><a class="repeat_btn" data-repeat="repeat">填充</a></li>
                                    </ul>
                                    <input type="hidden" value="不重复">
                                </div>
                            </div>
                        </div>
                    
                    <div class="change_color">
                        <strong><em>栏目文字</em></strong>
                        <p><input type="text" value="<%-commonjson.pageColor.textColor%>" class="colorwell color_radio_3"></p>
                    </div>
                    <div class="change_color bg_set bg_upload">
                        <div class="bg_color_img"><a class="on">背景颜色</a><a>背景图片</a></div>
                        <div class="bg_style"><p><input type="text" value="<%-commonjson.pageColor.backgroundColor%>" class="colorwell color_radio_4" id="backgroundColor"></p></div>
                        <div class="bg_style bgStyle_set">
