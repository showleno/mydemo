<div class="remove"><a class="clearMod">去除</a></div><i class="drag"></i>
<script>
            // 在这里请求数据，渲染页面
            $.ajax({
                type: 'get',
                url: 'http://www.daweiyuan.cn//br-isoc/drag/getdata',
                data: {
                    isocId: 1,
                    type: "news",
                    columnId: 1332,
                    count: 8
                }
            }).success(function (res) {
                console.log(res)
                var arr = res.contentList1332;
                $('.news_ul').html('');
                for ( var i = 0; i< arr.length; i++ ) {
                    var tpl = '';
                    tpl += '<li class="list_lst">';
                    tpl += '<span class="_ct"><i class="_dot"></i><a href="javascript" class="_link">'+arr[i].title+'</a></span>';
                    tpl += '</li>';
                    $('.news_ul').append(tpl);
                }
            }).error(function (err) {
                console.error(err);
            });
</script>