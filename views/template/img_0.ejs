<script>
    // 在这里请求数据，渲染页面
    $.ajax({
        type: 'get',
        url: 'http://www.daweiyuan.cn//br-isoc/drag/getdata',
        data: {
            isocId: 1,
            type: "do_pj",
            columnId: 1,
            count: 4
        }
    }).success(function (res) {
        console.log(res)
        var arr = res.donateProjectList;
        $('.img_ul').html('');
        for ( var i = 0; i< arr.length; i++ ) {
            var tpl = '';
            tpl += '<li class="list_lst">';
            tpl += '<div class="_cont">'
            tpl +=  '<img src="../index.jpg" class="_img_100 mr10"/>'
            tpl +=  '<div class="_rt">'
            tpl +=  '<p class="_name">'+arr[i].name+'</p>'
            tpl +=  '<p class="_dt" title="'+arr[i].details+'</p>'
            tpl +=  '</div>'
            tpl +=  '</div>'
            tpl += '</li>';
            $('.img_ul').append(tpl);
        }
    }).error(function (err) {
        console.error(err);
    });
</script>