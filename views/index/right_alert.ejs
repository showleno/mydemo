<h3><span>顶部设置</span></h3>
<div class="line_num routine"><a class="on percentage50" tab="0">纯色</a><a class="percentage50" tab="1">图像</a></div>
<div class="line_con" style="width:508px;right:0;">
    <div class="out scroll" style="margin-top:10px;">
        <div class="inner" style="width:194px;">
            <div class="pad_10">
                <div>
                    <div id="picker5"></div>
                    <div class="change_color">
                        <strong><em>背景颜色</em></strong>
                        <p><input type="text" value="<% if( topjson.bgcolor == "" ||  topjson.bgcolor == undefined ) { %> #fff <% } else { %> <%-topjson.bgcolor%>  <% }%>"  class="colorwell5 top_colorwell_0"></p>
                    </div>
                    <div class="change_color">
                        <strong><em>文字颜色</em></strong>
                        <p><input type="text" value="<% if( topjson.fontColor == "" ||  topjson.fontColor == undefined ) { %> #fff <% } else { %> <%-topjson.fontColor%>  <% }%>"  class="colorwell5 top_colorwell_1" ></p>
                    </div>
                    <div class="change_color">
                        <strong><em>按钮颜色</em></strong>
                        <p><input type="text" value="<% if( topjson.btnColor == "" ||  topjson.btnColor == undefined ) { %> #fff <% } else { %> <%-topjson.btnColor%>  <% }%>"  class="colorwell5 top_colorwell_2" ></p>
                    </div>
                    <div class="logo_set marTop20 wid70_p pad_0">
                        <div class="logo_size">
                            <div class="logo_modify_input">
                                <p>规格：</p>
                                <div class="select_down setWidth floatLeft marLeft10">
                                    <p class="wid48"><%-topjson.widthSize%><i class="fa fa-caret-down"></i></p>
                                    <ol>
                                        <li>宽屏</li>
                                        <li>窄屏</li>
                                    </ol>
                                    <input type="hidden" class="width" value="窄屏">
                                </div>
                            </div>
                            <div class="logo_modify_input">
                                <p>修改高度：</p>
                                <input type="text" class="marLeft10 modify_height" value="<%-topjson.height%>" onkeyup="value=value.replace(/[^\d]/g,'')";>
                            </div>
                            <div class="logo_modify_input">
                                <p>下间距(PX) ：</p>
                                <input type="text" class="marLeft10 modify_marBot"  value="<%-topjson.marBottom%>" onkeyup="value=value.replace(/[^\d]/g,'')";>
                            </div>
                            <div class="logo_modify_input">
                                <p>是否显示：</p>
                                <div class="select_down changeDisplay floatLeft marLeft10">
                                    <p class="wid48"><%-topjson.colorDoesShow[1]%><i class="fa fa-caret-down"></i></p>
                                    <ol>
                                        <li>是</li>
                                        <li>否</li>
                                    </ol>
                                    <input type="hidden" value="<%-topjson.colorDoesShow[1]%>" class="colorDoesShow">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="preservation_but"><a>保存</a></div>
            </div>
        </div>
    </div>
    <div class="out">
        <div class="inner" style="width:194px;">
            <div class="logo_set">
                <div class="up_img padd_7">
                    <div class="show_img"><i class="fa fa-camera"></i></div>
                    <div class="up_but"><label><a class="upload"><i class="fa fa-camera"></i>上传<input type="file" accept="image/gif,image/jpeg,image/jpg,image/png"></a></label><a  class="eliminate"><i class="fa fa-times"></i>清除</a></div>
                </div>
                <div class="bg_position marTop20">
                    <h3><span class="bg_283c50">背景位置</span></h3>
                    <div class="location_sele">
                        <a class="posi_top on"><em></em><input type="radio" name="bj_img_posi" checked></a>
                        <a class="posi_middle"><em></em><input type="radio" name="bj_img_posi"></a>
                        <a class="posi_bottom"><em></em><input type="radio" name="bj_img_posi"></a>
                    </div>
                </div>

                <div class="logo_set marTop20 wid70_p pad_0">
                    <div class="logo_size">
                        <div class="logo_modify_input">
                            <p>规格：</p>
                            <div class="select_down setWidth floatLeft marLeft10">
                                <p class="wid48"><%-topjson.widthSize%><i class="fa fa-caret-down"></i></p>
                                <ol>
                                    <li>宽屏</li>
                                    <li>窄屏</li>
                                </ol>
                                <input type="hidden" class="width" value="宽屏">
                            </div>
                        </div>
                        <div class="logo_modify_input">
                            <p>修改高度：</p>
                            <input type="text" class="marLeft10 modify_height" value="<%-topjson.height%>" onkeyup="value=value.replace(/[^\d]/g,'')";>
                        </div>
                        <div class="logo_modify_input">
                            <p>下间距(PX) ：</p>
                            <input type="text" class="marLeft10 modify_marBot" value="<%-topjson.marBottom%>"  onkeyup="value=value.replace(/[^\d]/g,'')";>
                        </div>
                        <div class="logo_modify_input">
                            <p>是否显示：</p>
                            <div class="select_down changeDisplay floatLeft marLeft10">
                                <p class="wid48"><%-topjson.imgDoesShow[1]%><i class="fa fa-caret-down"></i></p>
                                <ol>
                                    <li>是</li>
                                    <li>否</li>
                                </ol>
                                <input type="hidden" value="<%-topjson.imgDoesShow[1]%>" class="imgDoesShow">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="preservation_but"><a>保存</a></div>
        </div>
    </div>
</div>
</div>
<!-- banner -->
<div class="operation_panel layout right_number_6">
<h3><span>轮播图设置</span></h3>
